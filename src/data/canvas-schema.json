        "properties": {
          "id": {
            "$id": "/properties/containers/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              0
            ]
          },
          "title": {
            "$id": "/properties/containers/items/properties/title",
            "type": "string",
            "title": "The Title Schema ",
            "default": "",
            "examples": [
              "Key Partners"
            ]
          },
          "color": {
            "$id": "/properties/containers/items/properties/color",
