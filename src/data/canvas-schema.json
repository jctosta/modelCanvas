          "label": {
            "$id": "/properties/cards/items/properties/label",
            "type": "string",
            "title": "The Label Schema ",
            "default": "",
            "examples": [
              "Example card"
            ]
          },
          "description": {
            "$id": "/properties/cards/items/properties/description",
            "type": "string",
            "title": "The Description Schema ",
            "default": "",
